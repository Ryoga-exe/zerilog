const Reset = ResetAsyncLow;
const L8 = @Logic(.unsigned, 8);

pub module Counter(
    clk: input Clock,
    rst: input Reset,
    en:  input bit,
    q:   output @Logic(.unsigned, 8),
) {
    var r: logic8;

    always_ff {
        if_reset {
            r = 0;
        } else {
            r = r;
        }
    }
}
